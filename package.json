{
    "name": "@skeldjs/hindenburg",
    "version": "1.1.13",
    "main": "./dist/src/index",
    "bin": "./dist/bin/pkg.js",
    "license": "GPL-3.0-only",
    "scripts": {
        "build": "tsc -p ./",
        "setup": "ts-node ./bin/setup",
        "setup:dev": "cd scripts && yarn && node ./setupDev.js",
        "lint": "eslint src/**/*.ts",
        "fix": "eslint src/**/*.ts --fix",
        "plugins": "ts-node ./bin/plugins",
        "prepack": "yarn build",
        "publish": "yarn npm publish --access public",
        "start": "node ./dist/bin/worker",
        "dev": "ts-node ./bin/worker",
        "docs": "node scripts/writeConfigurationDocs.js && typedoc src/index.ts --out docs --json docs/json.json"
    },
    "devDependencies": {
        "@types/minimatch": "^3.0.5",
        "@types/node": "^15.12.2",
        "@types/polka": "^0.5.4",
        "@types/prompts": "^2.0.14",
        "@types/triple-beam": "^1.3.2",
        "@types/vorpal": "^1.12.0",
        "@types/yargs": "^17.0.0",
        "@typescript-eslint/eslint-plugin": "^5.27.0",
        "@typescript-eslint/parser": "^5.27.0",
        "eslint": "^8.17.0",
        "lint-staged": "^11.1.1",
        "pinst": "^2.1.6",
        "ts-node": "^10.0.0",
        "typedoc": "^0.21.2",
        "typedoc-plugin-pages": "patch:typedoc-plugin-pages@^1.1.0#./typedoc-plugin-pages-pr-41.diff",
        "typescript": "^4.3.2"
    },
    "dependencies": {
        "@schemastore/package": "^0.0.6",
        "@skeldjs/client": "^2.15.35",
        "@skeldjs/constant": "^2.15.35",
        "@skeldjs/core": "^2.15.35",
        "@skeldjs/data": "^2.15.35",
        "@skeldjs/events": "^2.15.35",
        "@skeldjs/protocol": "^2.15.35",
        "@skeldjs/reactor": "^2.15.35",
        "@skeldjs/state": "^2.15.35",
        "@skeldjs/text": "^2.15.35",
        "@skeldjs/util": "^2.15.35",
        "chalk": "^4.1.1",
        "chokidar": "^3.5.2",
        "compare-versions": "^3.6.0",
        "dotenv": "^10.0.0",
        "minimatch": "^3.0.4",
        "polka": "^0.5.2",
        "prompts": "^2.4.2",
        "query-registry": "^2.5.0",
        "reflect-metadata": "^0.1.13",
        "resolve-from": "^5.0.0",
        "resolve-pkg": "^2.0.0",
        "triple-beam": "^1.3.0",
        "vorpal": "^1.12.0"
    },
    "files": [
        "dist"
    ],
    "lint-staged": {
        "*.ts": "eslint src/**/*.ts"
    },
    "packageManager": "yarn@3.2.4",
    "pkg": {
        "assets": [
            "misc/config.schema.json",
            "build/yarn/bin/yarn.js",
            "build/yarn/lib/*"
        ]
    }
}
